// Debug switch cases
console.log("Testing switch cases");

var x: int64 = 2;
console.log("x =", x);

switch (x) {
    case 1:
        console.log("Case 1");
        break;
    case 2:
        console.log("Case 2");
        break;
    case 3:
        console.log("Case 3");
        break;
    default:
        console.log("Default case");
        break;
}

console.log("Done");